/* pages/user/account_to_edit/account_to_edit.wxss */
.forget{
  margin-top: 10rpx;
  background: #ffffff;
}
.forget .list{
  border-bottom: 1rpx solid #F7F6F6;
  display: flex;
  align-items: center;
}
.forget .list .title{
  color: #808080;
  width: 130rpx;
}
.forget .list .txt .username{
  width: 500rpx;
  overflow: hidden;
  text-overflow: ellipsis;
  white-space: nowrap;
}
.list_switch{
  display: flex;
  align-items: center;
  justify-content: space-between;
}
.list_switch .left{
  color: #009A44;
}
.info_btn button {
  background: #009a44;
  color: #fff;
  text-align: center;
  padding: 10rpx 0;
  border-radius: 10rpx;
  font-size: 28rpx;
  line-height: 1.2;
}